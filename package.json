{
  "name": "throwdowntv",
  "version": "1.0.0",
  "description": "A website built for streaming.",
  "main": "gruntfile.js",
  "scripts": {
    "dist-test": "grunt build-dist && cross-env NODE_ENV=\"prod\" node ./src/server/server.js",
    "prod": "grunt build-dist && cross-env NODE_ENV=\"prod\" pm2 start ./src/server/server.js",
    "dev": "cross-env NODE_ENV=\"dev\" grunt dev",
    "dev:lint": "eslint . --fix --ext .js"
  },
  "nodemonConfig": {
    "ignore": [
      "config/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThrowdownTV/ThrowdownTV.git"
  },
  "author": "DamienVesper",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ThrowdownTV/ThrowdownTV/issues"
  },
  "homepage": "https://github.com/ThrowdownTV/ThrowdownTV#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.0.0",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.17.1",
    "grunt": "^1.3.0",
    "grunt-concurrent": "^3.0.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-nodemon": "^0.4.2",
    "https": "^1.0.0",
    "mongoose": "^5.11.13",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "socket.io": "^3.1.0",
    "xss-filters": "^1.2.7"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1"
