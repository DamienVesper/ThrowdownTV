  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ThrowdownTV/ThrowdownTV/issues"
  },
  "homepage": "https://github.com/ThrowdownTV/ThrowdownTV#readme",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.3",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
