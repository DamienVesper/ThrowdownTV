{
  "name": "throwdowntv",
  "version": "1.0.0",
  "description": "A website built for streaming.",
  "main": "gruntfile.js",
  "scripts": {
    "build": "grunt build-dist",
    "dist-test": "cross-env NODE_ENV=\"prod\" node ./src/server/server.js",
